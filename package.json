{
  "name": "esjson",
  "version": "0.1.0",
  "description": "JSON Schema validator (cli)",
  "main": "src/index.js",
  "bin": {
    "esjson": "bin/esjson.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "lint": "eslint --ext .js .",
    "test": "uvu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Marcisbee/esjson.git"
  },
  "keywords": [
    "json",
    "schema",
    "validator",
    "fast",
    "cli"
  ],
  "author": "Marcis <marcisbergmanis@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Marcisbee/esjson/issues"
  },
  "homepage": "https://github.com/Marcisbee/esjson#readme",
  "devDependencies": {
    "eslint": "^7.13.0",
    "eslint-config-xo": "^0.33.1",
    "eslint-plugin-editorconfig": "^2.1.3",
    "eslint-plugin-import": "^2.22.1",
    "uvu": "^0.4.1"
  },
  "files": [
    "bin",
    "src"
  ],
  "dependencies": {
    "arg": "^4.1.3",
    "colorette": "^1.2.1"
  },
  "eslintConfig": {
    "extends": [
      "xo/esnext",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:editorconfig/noconflict"
    ],
    "plugins": [
      "import",
      "editorconfig"
    ],
    "rules": {
      "editorconfig/editorconfig": [
        "error",
        {
          "indent": {
            "SwitchCase": 1
          }
        }
      ]
    }
  }
}
